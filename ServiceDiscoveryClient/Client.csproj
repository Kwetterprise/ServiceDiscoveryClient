<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <nullable>enable</nullable>
    <PackageId>Kwetterprise.ServiceDiscovery.Client</PackageId>
    <Authors>Tim van den Essen</Authors>
    <Company>Kwetterprise</Company>
    <PackageDescription>A client library for the Kwetterprise service discovery server.</PackageDescription>
    <RepositoryUrl>https://github.com/Kwetterprise/ServiceDiscoveryClient</RepositoryUrl>
    <RootNamespace>Kwetterprise.ServiceDiscovery.Client</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.2.0" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="3.1.3" />
    <PackageReference Include="Microsoft.Extensions.Hosting.Abstractions" Version="3.1.3" />
  </ItemGroup>

</Project>
